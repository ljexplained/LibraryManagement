    <main id="main" class="main">

        <div class="pagetitle">
            <h1>Dashboard</h1>
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                    <li class="breadcrumb-item active">Admissions</li>
                </ol>
            </nav>
        </div><!-- End Page Title -->

        <section class="section">
            <div class="row">
                <div class="col-lg-12">

                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Admissions<span style="font-size:32px;">&nbsp;&nbsp;@ViewBag.totals Total</span></h5>
                            <hr />

                            <!-- Table with stripped rows -->
                            <table class="table datatable table-striped">
                                <thead>
                                    <tr>
                                        <th>
                                            S.N.

                                        </th>
                                        <th>
                                            <b>C</b>lass
                                        </th>
                                        <th>
                                            General<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            OBC<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            SC<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            ST<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            EWS<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            MINORITY (Muslims)<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            MINORITY (Sikh)<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            MINORITY (Jain)<br />
                                            <table class="table table-bordered">
                                                <tr><th>M</th><th>F</th><th>O</th></tr>
                                            </table>
                                        </th>
                                        <th>
                                            Total
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @if(Students!=null)
                                    {

                                        foreach(var item in Students)
                                        {


                                            <tr>
                                                <td>#</td>
                                                <td>@item.course</td>

                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.genm</td><td>@item.genf</td><td>@item.geno</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.obcm</td><td>@item.obcf</td><td>@item.obco</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.scm</td><td>@item.scf</td><td>@item.sco</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.stm</td><td>@item.stf</td><td>@item.sto</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.ewm</td><td>@item.ewf</td><td>@item.ewo</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.minm_m</td><td>@item.minm_f</td><td>@item.minm_o</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.mins_m</td><td>@item.minos_f</td><td>@item.minos_o</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.minj_m</td><td>@item.minj_f</td><td>@item.minj_o</td></tr>
                                                    </table>
                                                </td>
                                                <td>
                                                    <table class="table table-bordered">
                                                        <tr><td>@item.total</td></tr>
                                                    </table>
                                                </td>
                                            </tr>
                                            
                                        }
                                    }

                                

                                </tbody>
                               
                            </table>
                            <!-- End Table with stripped rows -->

                            <a style="float:right;"  href="~/Collegeadmin/Admissionlogin" class="btn btn-warning">Next</a>

                        </div>
                    </div>


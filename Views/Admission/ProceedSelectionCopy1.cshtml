            <div class="container-fluid pt-4 px-4">
                <form asp-action="UpdateProceedSelection2" asp-controller="Admission" method="post">
                    <div class="row g-4">
                         
                       
                        <div class="col-sm-12 col-xl-11">
                            <div class="bg-light rounded h-100 p-4">
                                <h6 class="mb-4">Under Graduates</h6>

                                


                                <div id="" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Year</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if(ViewBag.ugyear!=null){
                                        @ViewBag.ugyear
}" name="ugpassyear" class="form-control" />
                                    </div>


                                </div>



                                <div  class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Marks Obtained</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if(ViewBag.ugmarks!=null){
                                        @ViewBag.ugmarks
}" inputmode="decimal" pattern="[0-9]*[.,]?[0-9]*" name="ugmarks" class="form-control" />
                                    </div>


                                </div>



                                <div class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Max Marks</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if(ViewBag.ugmax!=null){
                                        @ViewBag.ugmax
}" inputmode="decimal" pattern="[0-9]*[.,]?[0-9]*" name="ugmaxmarks" class="form-control" />
                                    </div>


                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">Division</label>
                                    <div class="col-sm-6">

                                        <input type="text" value="@if(ViewBag.ugdiv!=null){
                                        @ViewBag.ugdiv
}" name="ugdiv" class="form-control" />
                                        
                                    </div>
                                </div>



                                <div id="" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Percentage(%)</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if(ViewBag.ugper!=null){
                                        @ViewBag.ugper
}" inputmode="decimal" pattern="[0-9]*[.,]?[0-9]*" name="ugpercent" class="form-control" />
                                    </div>


                                </div>





                                <div class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Group</label>
                                    <div class="col-sm-6">
                                        <textarea name="uggroup"  class="form-control">@if(ViewBag.uggp != null) { @ViewBag.uggp}</textarea>
                                    </div>


                                </div>



                                <div id="" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">School/College</label>
                                    <div class="col-sm-6">
                                        <textarea name="ugcollege" class="form-control">@if (ViewBag.ugschool != null){@ViewBag.ugschool}</textarea>
                                    </div>


                                </div>


                                <div class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Board/University</label>
                                    <div class="col-sm-6">
                                        <textarea name="ugunivercity" class="form-control">@if (ViewBag.ugboard != null){@ViewBag.ugboard}</textarea>
                                    </div>


                                </div>




                              


                            </div>
                        </div>



                       



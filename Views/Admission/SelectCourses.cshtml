    <script>

        $('#level_id').change(function () {

            var item = document.getElementById("level_id").value;
            $.ajax({
                url: '/Admission/SelectCourses',
                type: 'post',
                datatype: 'html',
                data: { 'levelnm': item},
                success: function (result) {

                    //$('#Course-List').html(result);
                    $('#course_id').html(result);
                    // alert(result);

                },
                error: function () {

                }


            });




        });

    </script>

    <script>

        $('#course_id').change(function () {

            var item1 = document.getElementById("course_id").value;

            //alert(item1);

            $.ajax({
                url: '/Admission/SelectCourses',
                type: 'post',
                datatype: 'html',

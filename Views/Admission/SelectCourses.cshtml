                    <div class="row g-4">

                       <div class="col-sm-12 col-xl-8">

                          <form asp-action="UpdateSelectCourses" asp-controller="Admission" method="post">
                            <div class="bg-light rounded h-100 p-4">
                             
                                <h4>Registration</h4>

                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">Level</label>
                                    <div class="col-sm-6">
                                        <Select name="lname" required="" type="text" class="form-control" id="level_id">
                                            @* <option>
                                              --Select--
                                            </option> *@
                                            <option>
                                              UG
                                            </option>
                                            <option>
                                                PG
                                            </option>

                                            <option>
                                                PhD
                                            </option>
                                            </Select>
                                    </div>
                                </div>
                                <div id="Course-List" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Courses</label>
                                    <div class="col-sm-6">
                                        <Select name="Cname" required="" type="text" class="form-control" id="course_id">
                                            
                                            <option>
                                               BA(Aided)
                                            </option>
                                            <option>
                                                BCA
                                            </option>

                                            <option>
                                                B.Com
                                            </option>
                                             <option>
                                                Bsc
                                            </option>
                                            <option>
                                                BEd
                                            </option>
                                            <option>
                                                BA(Self Finance)
                                            </option>
                                        </Select>
                                    </div>


                                </div>


                                <div id="year-List" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Academic Year</label>
                                    <div class="col-sm-6">
                                        <Select name="Cacadyear" required="" type="text" class="form-control" id="">

                                            <option>
                                              1st year
                                            </option>
                                             <option>
                                              2nd year
                                            </option>
                                            <option>
                                                3rd year
                                            </option>
                                            
                                        </Select>
                                    </div>


                                </div>



                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">University Registration/Enrollment No</label>
                                    <div class="col-sm-6">
                                        <input type="text" id="" name="unenroll" class="form-control">
                                    </div>
                                </div>
                                <div id="" class="row mb-3">

                                    <label class="col-sm-6 col-form-label">Admission Category</label>
                                    <div class="col-sm-6">
                                        <select type="text" id="" name="admcategory" class="form-control">
                                            <option>Gen</option>
                                            <option>OBC</option>
                                            <option>SC</option>
                                            <option>ST</option>
                                            <option>EWS</option>
                                            <option>Minority (Muslim)</option>
                                            <option>Minority (Sikh)</option>
                                            <option>Minority (Jain)</option>
                                        </select>
                                    </div>

                                   

                                </div>


                                <div id="year-List" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Merit</label>
                                    <div class="col-sm-6">
                                        <select type="text" id="" name="unmerit" class="form-control">
                                            <option>None</option>
                                            <option>1st</option>
                                            <option>2nd</option>
                                            <option>3rd</option>
                                            <option>Open Merit 1st</option>
                                            <option>Open Merit 2nd</option>
                                        </select>
                                    </div>


                                </div>



                                <div class="col-sm-12 col-xl-6">
                                    <button  type="submit" class="btn btn-primary">Save and Proceed</button>
                                </div>

                            </div>

                          </form>
                         


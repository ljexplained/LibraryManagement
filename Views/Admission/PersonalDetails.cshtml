            <!-- Form Start -->
            <div class="container-fluid pt-4 px-4">
                <form asp-action="UpdatePersonalDetails" asp-controller="Admission" method="post">
                    <div class="row g-4">

                        <div class="col-sm-12 col-xl-11">
                            <div class="bg-light rounded h-100 p-4">
                                @* <h6 class="mb-4">Admission Form</h6> *@

                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">Candidate Name</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if(ViewBag.name!=null){@ViewBag.name}" id="" name="candidate" class="form-control">
                                    </div>
                                </div>
                                <div id="" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Father's Name</label>
                                    <div class="col-sm-6">
                                        <input type="text" id="" value="@if(ViewBag.fatname!=null){
                                        @ViewBag.fatname
}" name="fathername" class="form-control">
                                    </div>


                                </div>


                                <div id="year-List" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Gaurdian's Name</label>
                                    <div class="col-sm-6">
                                        <input type="text" id="" value="@if(ViewBag.gauname!=null){
                                        @ViewBag.gauname
}" name="gaurdian" class="form-control">
                                    </div>


                                </div>




                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">Mother's Name</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if(ViewBag.mothname!=null){@ViewBag.mothname}"

              id="" name="mothername" class="form-control">
                                    </div>
                                </div>
                                <div id="" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Date Of Birth</label>
                                    <div class="col-sm-6">
                                        <input type="text" value="@if (ViewBag.dob != null)
                                        {
                                            @ViewBag.nation
                                        }"

                                            id="" name="dob" class="form-control">
                                    </div>


                                </div>
                                <div id="year-List" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Nationality</label>
                                    <div class="col-sm-6">
                                        <input type="text" id="" value="@if (ViewBag.nation!= null)
                                               {
                                                  @ViewBag.nation
                                               }
"
                                     name="nation" class="form-control">
                                    </div>


                                </div>


                                <div id="" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Gender</label>
                                    <div class="col-sm-6">
                                        <Select type="text" id="" name="gender" class="form-control">
                                            @if(ViewBag.gender!=null)
                                            {
                                                <option>@ViewBag.gender</option>
                                            }
                                            <option>Male</option>
                                            <option>Female</option>
                                            <option>Others</option>
                                        </Select>
                                    </div>


                                </div>


                                <div id="year-List" class="row mb-3">



                                    <label class="col-sm-6 col-form-label">Category</label>
                                    <div class="col-sm-6">
                                        <select type="text" id="" name="Castecategory" class="form-control">
                                            @if(ViewBag.categ!=null)
                                            {
                                                <option>@ViewBag.categ</option>
                                            }
                                            <option>Gen</option>
                                            <option>OBC</option>
                                            <option>SC</option>
                                            <option>ST</option>
                                            <option>EWS</option>
                                        <option>Minority (Muslim)</option>
                                            <option>Minority (Sikh)</option>
                                            <option>Minority (Jain)</option>
                                        </select>
                                    </div>


                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">Sub Category</label>
                                    <div class="col-sm-6">
                                        <select type="text" id="" name="Subcategory" class="form-control">
                                            @if(ViewBag.scateg!=null)
                                            {
                                               <option>@ViewBag.scateg</option>
                                            }
                                            <option>None</option>
                                            <option>PH</option>
                                            <option>Ex.Ser/DEP</option>
                                            <option>DEP FF</option>

                                        </select>
                                    </div>
                                </div>




                            </div>
                        </div>

                       
                        @if(ViewBag.permit!=null)
                        {
                            <div class="col-sm-12 col-xl-6">
                                <a href="~/Admission/ProceedSelection" class="btn btn-primary">Next</a>
                            </div>
                        }
                        else
                        {
                            <div class="col-sm-12 col-xl-6">
                                <button type="submit" class="btn btn-primary">Save and Proceed</button>
                            </div>
                            
                        }

                       




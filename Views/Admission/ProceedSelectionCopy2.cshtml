            <!-- Form Start -->
            <div class="container-fluid pt-4 px-4">
                <form  enctype="multipart/form-data" asp-action="UpdateProceedSelection3" asp-controller="Admission" method="post">
                    <div class="row g-4">

                       
                        <div class="col-sm-12 col-xl-12">
                            <div class="bg-light rounded h-100 p-4">
                               @*  <h6 class="mb-4">Admission Form</h6> *@

                                <div class="row mb-3">

                                    <p style="color:red;">1. if uploading the documents first time then select the files which you want to upload
                                        and then press save and proceed.
                                    </p>

                                    <p style="color:red;">
                                        2. if already  uploaded then do not press (save and proceed) button, you can press skip button. 
                                    </p>

                                    <p style="color:red;">
                                        3. if already  uploaded and want to upload again  then also upload the documents that is already uploaded.
                                    </p>

                                </div>
                               

                                <div  class="row mb-3">



                                    <label class="col-sm-6 col-form-label">1. University Reg. Form</label>
                                    <div class="col-sm-6">
                                        <input type="file" name="file1" class="form-control" />
                                    </div>


                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">2. Offer Letter</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file2" class="form-control" />
                                        
                                    </div>
                                </div>



                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">3. 10th Marksheet</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file3" class="form-control" />

                                    </div>
                                </div>

                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">4. 10th Certificate</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file4" class="form-control" />

                                    </div>
                                </div>



                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">5. 12th Marksheet</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file5" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">6. 12th Certificate</label>
                                    <div class="col-sm-6">

                                        <input type="file" value="rahul.png" name="file6" class="form-control" />

                                    </div>
                                </div>

                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">7. UG/PG Marksheets </label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file7" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">8. Migration </label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file8" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">9. T.C </label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file9" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">10. Character Certificate</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file10" class="form-control" />

                                    </div>
                                </div>



                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">11. Caste Certificate</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file11" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">12. Aadhaar Card</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file12" class="form-control" />

                                    </div>
                                </div>



                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">13. Domicile Certificate</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file13" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">14. Weightage Certificate(if any)</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file14" class="form-control" />

                                    </div>
                                </div>



                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">15. Bank Pass Book</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file15" class="form-control" />

                                    </div>
                                </div>


                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">16. Signature</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file16" class="form-control" />

                                    </div>
                                </div>

                                <div class="row mb-3">
                                    <label class="col-sm-6 col-form-label">17. Photograph</label>
                                    <div class="col-sm-6">

                                        <input type="file" name="file17" class="form-control" />

                                    </div>
                                </div>



                            </div>
                        </div>




                        @if (ViewBag.permit != null)
                        {
                            <div class="col-sm-12 col-xl-6">
                                <a href="~/Admission/ProceedSelectionCopy3" class="btn btn-primary">Next</a>
                            </div>
                        }
                        else
                        {
                            <div class="col-sm-12 col-xl-6">
                                <button type="submit" class="btn btn-primary">Save and Proceed</button>
                            </div>

                        }



                    </div>

                </form>
            </div>
            <!-- Form End -->
            <!-- Footer Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="bg-light rounded-top p-4">
